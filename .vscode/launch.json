    "version": "0.2.0",
    "configurations": [
        {
            "name": "resume - Flutter web",
            "cwd": "resume",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "../resume/lib/main.dart",
            "toolArgs": [
                "--web-browser-flag=--disable-web-security",
                "--web-browser-flag=--user-data-dir=/tmp/chrome_dev"
